    "25": [
        { "content": "“Speak to Aaron and to his sons, saying, ‘This is the law of the sin offering: in the place where the burnt offering is killed, the sin offering shall be killed before the" },
        { "content": "LORD", "strongs": "H3068", "token": "17" },
        { "content": ". It is most holy." }
    ],
    "26": [
        { "content": "The priest who offers it for sin shall eat it. It shall be eaten in" },
        { "content": "a", "strongs": "H3068" },
        { "content": "holy place, in the court of the Tent of Meeting." }
    ],
    "27": [
        { "content": "Whatever shall touch its flesh shall be holy. When there is any of its blood sprinkled on" },
        { "content": "a", "strongs": "H3068" },
        { "content": "garment, you shall wash that on which it was sprinkled in" },
        { "content": "a", "strongs": "H3068" },
        { "content": "holy place." }
    ],
    "28": [
        { "content": "But the earthen vessel in which it is boiled shall be broken; and if it is boiled in" },
        { "content": "a", "strongs": "H3068" },
        { "content": "bronze vessel, it shall be scoured, and rinsed in water." }
    ],
    "29": [
        { "content": "Every male amongst the priests shall eat of it. It is most holy." }
    ],
    "30": [
        { "content": "No sin offering, of which any of the blood is brought into the Tent of Meeting to make atonement in the Holy Place, shall be eaten. It shall be burnt with fire." }
    ]
